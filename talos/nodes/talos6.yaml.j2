machine:
  type: worker
---
apiVersion: v1alpha1
kind: HostnameConfig
auto: "off"
hostname: talos6.vaderrp.com
---
apiVersion: v1alpha1
kind: LinkConfig
name: eth0
mtu: 1500
addresses:
  - address: 192.168.7.26/24
routes:
  - gateway: 192.168.7.1
  - destination: 192.168.0.0/16
    gateway: 192.168.7.1
---
apiVersion: v1alpha1
kind: LinkConfig
name: eth1
mtu: 1500
---
apiVersion: v1alpha1
kind: UserVolumeConfig
name: local-storage
provisioning:
  diskSelector:
    match: '!system_disk'
  minSize: 32GB
